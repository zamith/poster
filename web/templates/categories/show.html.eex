<h2>Show category</h2>

<ul>

    <li>
        <strong>Name:</strong>
        <%= @category.name %>
    </li>

    <li>
        <strong>Description:</strong>
        <%= @category.description %>
    </li>

</ul>

<%= link "Edit", to: categories_path(@conn, :edit, @category) %>
<%= link "Back", to: categories_path(@conn, :index) %>
